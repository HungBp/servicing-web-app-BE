{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "quotes": ["warn", "double"],
        "max-len": "off",
        "no-console": "off",
        "arrow-parens": "off",
        "comma-dangle": "off",
        "no-unused-vars": "warn",
        "no-plusplus": "off",
        "dot-notation": "off",
        "no-use-before-define": "off"
    }
}
